{
    "name": "konekt/bearer-auth",
    "description": "RFC 6750 Compatible Bearer Authentication for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["api", "bearer", "rfc-6750", "oauth2", "konekt", "artkonekt", "laravel", "rest"],
    "support": {
        "source": "https://github.com/artkonekt/bearer-auth"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^8.1",
        "lcobucci/jwt": "^4.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "konekt/concord": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": { "Konekt\\BearerAuth\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\BearerAuth\\Tests\\": "tests/"
        }
    }
}
